{
  "name": "universal-agent-connector",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for Universal Agent Connector - AI agent management and database integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run build:esm",
    "build:esm": "tsc --module esnext --outDir dist/esm && rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "database",
    "connector",
    "sql",
    "llm",
    "openai",
    "anthropic",
    "typescript",
    "sdk"
  ],
  "author": "Universal Agent Connector Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/universal-agent-connector/javascript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/universal-agent-connector/javascript-sdk/issues"
  },
  "homepage": "https://github.com/universal-agent-connector/javascript-sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}

