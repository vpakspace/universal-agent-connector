  <!-- ============================================================ -->
  <!-- Section 1: Ontology Metadata                                  -->
  <!-- ============================================================ -->
  <owl:Ontology rdf:about="http://ontology.example.org/finance">
    <rdfs:label>Finance Domain Ontology</rdfs:label>
    <rdfs:comment>OWL ontology for banking and financial services RBAC.
Defines roles, entities, actions and access control rules for OntoGuard semantic validation.</rdfs:comment>
    <owl:versionInfo>1.0.0</owl:versionInfo>
  </owl:Ontology>

  <!-- ============================================================ -->
  <!-- Section 2: Base Classes                                       -->
  <!-- ============================================================ -->
  <owl:Class rdf:about="http://ontology.example.org/finance#Role">
    <rdfs:label>Role</rdfs:label>
    <rdfs:comment>Base class for all roles in the finance domain</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://ontology.example.org/finance#Entity">
    <rdfs:label>Entity</rdfs:label>
    <rdfs:comment>Base class for all entities in the finance domain</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://ontology.example.org/finance#Action">
    <rdfs:label>Action</rdfs:label>
    <rdfs:comment>Base class for all actions in the finance domain</rdfs:comment>
  </owl:Class>

