    <rdfs:range rdf:resource="http://ontology.example.org/finance#Role"/>
    <rdfs:label>requiresRole</rdfs:label>
    <rdfs:comment>Links an action rule to the role(s) that can perform it</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://ontology.example.org/finance#appliesTo">
    <rdfs:domain rdf:resource="http://ontology.example.org/finance#Action"/>
    <rdfs:range rdf:resource="http://ontology.example.org/finance#Entity"/>
    <rdfs:label>appliesTo</rdfs:label>
    <rdfs:comment>Links an action rule to the entity type it applies to</rdfs:comment>
  </owl:ObjectProperty>

