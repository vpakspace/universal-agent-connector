    <rdfs:range rdf:resource="http://ontology.example.org/finance#Entity"/>
    <rdfs:label>appliesTo</rdfs:label>
    <rdfs:comment>Links an action rule to the entity type it applies to</rdfs:comment>
  </owl:ObjectProperty>

